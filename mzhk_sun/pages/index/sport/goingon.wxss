page{
    font-size: 16px;
}
.board{
    width: 720rpx;
    margin: 0px 15rpx;
}
.pintu{
    width: 100%;
    height: 600rpx;
    /* background: #b3b3b3; */
    margin-top: 20rpx;
}
.area{
    float: left;
    margin-right:10rpx;
    margin-bottom: 10rpx;
    background:rgb(209, 188, 230);
    padding: 3rpx 20rpx;
}

.cg3{
    border-radius: 0 0 10px 10px;
    clear: both;
}
.cg4{
    background: #fff;
    height: 280rpx;
}
.test{
    width: 756rpx;
    background: linear-gradient(180deg, #ffffff, #ff000000);
    clear: both;
    float: left;
    height: 118rpx;
}
.plantit{
    background: #F44336;
    color: #fff;
    padding: 15rpx;
    font-size: 14px;
}
.title {
    width: 50rpx;
    height: 50rpx;
    background: #fff;
    float: left;
    box-sizing: border-box;
    text-align: center;
    vertical-align: middle;
    line-height: 50rpx;
    font-size: 12px;
    margin-top: 11rpx;
    margin-right: 10rpx;
    border-radius: 5px;
}
/* 环绕造型 */
.dot {
    width: 85rpx;
    /* #fff807 */
    background: #fffc98;
    /* background: #e2e2e2; */
    float: left;
    box-sizing: border-box;
    text-align: center;
    vertical-align: middle;
    line-height: 40rpx;
    font-size: 12px;
    margin-right: 30.5rpx;
    margin-bottom: 13rpx;
    border-radius: 15px 0;
}
.dot:nth-child(6n){
    margin-right: 0;
}
.check{
    width: 35rpx;
    height: 35rpx;
    padding-top: 13rpx;
}
.distance{
    height: 9rpx;
    /* background: #07ff25; */
}
.feng{
    width: 95%;
    margin: 0 auto;
    box-sizing: border-box;
    background: #fff;
    padding-left: 0;
    padding-top: 0;
    margin-top: 16px;

    border-left: none;
    margin-bottom: 15rpx;
}
.bian{
    width: 50rpx;
    float: left;
}

.bei{
    width: 100%;
    padding: 3.5% 0 1%;
    box-sizing: border-box;
}
.prog{
    height: 50rpx;
    /* background-color: #fbff18; */
}
/* 列表 */
.lists{
    clear: both;
}
.list{
    background: #e0e0e0;
    margin-bottom: 21rpx;
    font-size: 14px;
}
.detailgroup{
    padding: 10rpx;
}
.details{
    display: flex;
    margin-bottom: 10rpx;
}
.details view{
    margin-right: 10rpx;
}
.detail1{
    background: #e25151;
}
.detail2{
    background: #51e2aa;
}
.detail3{
    background: #9551e2;
}
.detail4{
    display: inline;
    border:1px solid #fffc98;
    margin-bottom: 10rpx;
    padding: 0 5rpx;
}
.detail5{
    display: inline;
    border:1px solid #fffc98;
    padding: 0 5rpx;
}
.map{
    display: block;
    margin-top: 12rpx;
}
.team{
    clear: both;
    display: flex;
    align-items: flex-end;
    padding: 15rpx 0 15rpx;
    flex-wrap: wrap;
    width: 683rpx;
    border-bottom: 1px solid #d0d0d0;
}
.blank{
    height: 65rpx;
    width: 65rpx;
    margin-right: 16rpx;
    border-radius: 7px;
    background: #fff;
    border: 1px solid #d0d0d0;
    margin-bottom:16rpx;
    overflow: hidden;
}
.blank:nth-child(8){
    margin-right: 0
}
.blank image{
    height: 65rpx;
    width: 65rpx;
}
.more{
    padding: 10rpx;
    text-align: center;
    background: chartreuse;
}
.hideblock{
    /* transform:rotate(7deg); */
    transform: skew(90%);
    /* width:100px;
    transition: width 2s; */
}

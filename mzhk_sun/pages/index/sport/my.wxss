page{
    font-size: 16px;
    background:#dedede;
}
.centre{
    width: 750rpx;
    background: #00e6c6;
    background: #98e7ff;
    display: flex;
    padding: 60rpx 62rpx 55rpx;
    box-sizing: border-box;
    align-items: center;
}
.user{
    padding-left: 47rpx;
    color: #fff;
}
.user_info{
    display: flex;
    align-items: center;
}
.level{
    display: flex;
    height: 85rpx;
    /* background: chartreuse; */
    align-items: center;
    margin-left: 5rpx;
}
.leicon{
    display: flex;
    align-items: center;
}
.leicon image{
    height: 50rpx;
    width: 50rpx;
    border-radius: 50%;
    z-index: 10;
}
.level{
    font-size: 14px;
}
.grade{
    /* margin-left: -22rpx; */
    background: #ff4f4f;
    /* height: 50rpx; */
    /* line-height: 50rpx; */
    /* padding: 0 13rpx 0 30rpx; */
    /* border-radius: 63px; */
    width: 63rpx;
    text-align: center;
}
.tel{
    font-size: 12px;
}
.headimg {
    height: 100rpx;
    width: 100rpx;
    /* padding: 11rpx;
    background: #8dfff4;
    background: #fbfbfb; */
    border-radius: 50%;
    overflow: hidden;
}
.headimg image{
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
.board{
    width: 96%;
    padding: 15rpx 15rpx;
    box-sizing: border-box;
    background: #fff;
    margin: 0 auto;
    margin-top: 28rpx;
}
.lists{
    width: 100%;
}
.li{
    width: 100%;
    height: 98rpx;
    line-height: 98rpx;
    display: flex;
    align-items: center;
}
.list{
    border-bottom: 1px solid #ccc;
}
.list:last-child{
    border: none;
}
.mark{
    width: 50rpx;
    height: 50rpx;
    margin-left: 20rpx;
    display: none;
}
.tit{
    font-size: 14px;
    padding-left: 25rpx;
}